extends ../layout.jade
block cabecera
	h2(class='pacifico big blue') Mis Series
	p Las mejores series
	each producto in productos
		div(class="col-md-4 col-sm-6 col-xs-12 inline-block")
			h3(class="pacifico")= producto.titulo
			p(style="margin-top:1em;") #[small(class="lato small") #{producto.nota}"/"10]